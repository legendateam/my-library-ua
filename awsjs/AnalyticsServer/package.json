{
  "name": "analyticsserver",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "rimraf dist && copyfiles -f ./src/email-templates/*.* ./dist/email-templates && copyfiles -f ./src/docs/*.json ./dist/docs/ && tsc-watch --onSuccess \"npm run watch:server\"",
    "watch:server": "nodemon --watch src ./src/app.ts",
    "start:prod": "node app.js",
    "migration:run": "typeorm migration:run -d ./configs/orm.config.js",
    "migration:create": "typeorm migration:create",
    "migration:revert": "typeorm-ts-node-commonjs -d ./src/configs/ormconfig.ts migration:revert",
    "migration:generate": "typeorm-ts-node-commonjs -d ./src/configs/ormconfig.ts migration:generate"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "copyfiles": "^2.4.1",
    "cors": "^2.8.5",
    "dayjs": "^1.11.6",
    "email-templates": "^10.0.1",
    "express": "^4.18.2",
    "ip": "^2.0.0",
    "joi": "^17.6.0",
    "mongoose": "^6.7.2",
    "node-cron": "^3.0.2",
    "nodemailer": "^6.8.0",
    "nodemon": "^2.0.20",
    "pg": "^8.7.3",
    "pug": "^3.0.2",
    "redis": "^4.2.0",
    "swagger-ui-express": "^4.6.0",
    "typeorm": "^0.3.6"
  },
  "devDependencies": {
    "@typegoose/typegoose": "^9.12.1",
    "@types/copyfiles": "^2.4.1",
    "@types/cors": "^2.8.12",
    "@types/dotenv": "^8.2.0",
    "@types/email-templates": "^10.0.1",
    "@types/express": "^4.17.14",
    "@types/ip": "^1.1.0",
    "@types/joi": "^17.2.3",
    "@types/node": "^18.11.9",
    "@types/node-cron": "^3.0.6",
    "@types/nodemailer": "^6.4.6",
    "@types/pug": "^2.0.6",
    "@types/redis": "^4.0.11",
    "@types/swagger-ui-express": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "@typescript-eslint/parser": "^5.43.0",
    "eslint": "^8.27.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.26.0",
    "rimraf": "^3.0.2",
    "ts-node": "^10.9.1",
    "tsc-watch": "^5.0.3"
  }
}
